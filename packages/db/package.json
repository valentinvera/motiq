{
  "name": "@motiq/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./schema/auth": {
      "types": "./dist/schema/auth.d.mts",
      "import": "./dist/schema/auth.mjs"
    },
    "./schema/waitlist": {
      "types": "./dist/schema/waitlist.d.mts",
      "import": "./dist/schema/waitlist.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@motiq/env": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "@neondatabase/serverless": "^1.0.1",
    "ws": "^8.18.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.2",
    "@types/ws": "^8.18.1",
    "@motiq/ts-config": "workspace:*",
    "tsdown": "catalog:"
  }
}
