{
  "name": "@motiq/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "src/styles"
  ],
  "exports": {
    "./globals.css": "./src/styles/globals.css",
    "./utils": {
      "types": "./dist/lib/utils.d.mts",
      "import": "./dist/lib/utils.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.mts",
      "import": "./dist/components/*.mjs"
    },
    "./icons/*": {
      "types": "./dist/icons/*.d.mts",
      "import": "./dist/icons/*.mjs"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.mts",
      "import": "./dist/hooks/*.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "dist/lib/utils.d.mts"
      ],
      "components/*": [
        "dist/components/*.d.mts"
      ],
      "icons/*": [
        "dist/icons/*.d.mts"
      ],
      "hooks/*": [
        "dist/hooks/*.d.mts"
      ]
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.36.1",
    "@tsparticles/engine": "^3.9.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.568.0",
    "motion": "^12.29.2",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@motiq/ts-config": "workspace:*",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "tsdown": "catalog:"
  }
}
